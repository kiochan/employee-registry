{
  "private": true,
  "name": "employee-registry",
  "version": "0.0.3",
  "description": "simple system for managing data of employee",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "commit": "pnpm commit:pre && git-cz",
    "release": "standard-version",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint \"./**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint --fix \"./**/*.{js,jsx,ts,tsx}\"",
    "format": "pnpm lint:fix && prettier --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" --config prettier.config.js",
    "commit:pre": "pnpm lint && pnpm format"
  },
  "keywords": [
    "react",
    "typescript"
  ],
  "author": {
    "name": "Xingchen Sun",
    "email": "sunxingchen@live.com",
    "url": "https://github.com/kiochan"
  },
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "devDependencies": {
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.8",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "git-cz": "^4.9.0",
    "prettier": "^2.7.1",
    "standard-version": "^9.5.0",
    "typescript": "*"
  },
  "dependencies": {
    "next": "13.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  }
}
